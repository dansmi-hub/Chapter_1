@article{homburgCarabidsOrgDynamic2014,
  title = {Carabids.Org \textendash{} a Dynamic Online Database of Ground Beetle Species Traits ({{Coleoptera}}, {{Carabidae}})},
  author = {Homburg, Katharina and Homburg, Nils and Sch{\"a}fer, Florian and Schuldt, Andreas and Assmann, Thorsten},
  year = {2014},
