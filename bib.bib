@article{ejiriEntomologicalStudyTransmission2011,
  title = {Entomological Study on Transmission of Avian Malaria Parasites in a Zoological Garden in {{Japan}}: {{Bloodmeal}} Identification and Detection of Avian Malaria Parasite {{DNA}} from Blood-Fed Mosquitoes},
  shorttitle = {Entomological Study on Transmission of Avian Malaria Parasites in a Zoological Garden in {{Japan}}},
