  shorttitle = {Fauna and {{Larval Habitats}} of {{Mosquitoes}} ({{Diptera}}},
  author = {{Khoshdel-Nezamiha}, Farahnaz and Vatandoost, Hassan and {Azari-Hamidian}, Shahyad and Bavani, Mulood Mohammadi and Dabiri, Farrokh and {Entezar-Mahdi}, Rasool and Chavshin, Ali Reza},
