@article{randolphHumanActivitiesPredominate2010,
  title = {Human Activities Predominate in Determining Changing Incidence of Tick-Borne Encephalitis in {{Europe}}},
  author = {Randolph, S. E. and sub-project Team, Collective on behalf of the EDEN-TBD},
  year = {2010},
